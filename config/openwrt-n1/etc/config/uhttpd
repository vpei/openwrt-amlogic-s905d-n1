
config uhttpd 'main'
	list listen_http '0.0.0.0:80'
	list listen_http '[::]:80'
	option home '/www'
	option max_connections '100'
	option cgi_prefix '/cgi-bin'
	option script_timeout '4800'
	option network_timeout '2400'
	option http_keepalive '20'
	option tcp_keepalive '1'
	option ubus_prefix '/ubus'
	list index_page 'cgi-bin/luci'
	option max_requests '50'
	option rfc1918_filter '0'
	list listen_https ' 0.0.0.0:443'
	list listen_https '[::]:443'
	option cert '/etc/config/https/vmess.com.crt'
	option key '/etc/config/https/vmess.com.key'
	option redirect_https '0'
	option lua_prefix '/cgi-bin/luci=/usr/lib/lua/luci/sgi/uhttpd.lua'

config cert 'defaults'
	option days '730'
	option key_type 'ec'
	option bits '2048'
	option ec_curve 'P-256'
	option state 'Somewhere'
	option country 'CN'
	option location 'HK'
	option organization 'TW'
	option commonname 'C-NET'

